    "# 2004: 조합 0의 개수\n",
    "\n",
    "n,m = map(int, input().split())\n",
    "\n",
    "def Count(a,b):\n",
    "    c = 0\n",
    "    while a > 0:\n",
    "        a = a//b\n",
    "        c += a\n",
    "    return c\n",
    "\n",
    "cnt_2 = Count(n,2) - (Count(m,2)+Count(n-m,2))\n",
    "cnt_5 = Count(n,5) - (Count(m,5)+Count(n-m,5))\n",
    "\n",
    "print(min(cnt_2, cnt_5))"
   ]
  }
